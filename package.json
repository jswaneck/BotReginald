    "deploy": "npm run build && node ./dist/deploy-commands.js",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jswaneck/BotReginald.git"
  },
  "keywords": [
