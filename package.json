    "@types/node-fetch": "2.5.12",
    "@types/node-schedule": "1.3.2",
    "@types/pino": "6.3.11",
    "@types/remove-markdown": "0.3.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
