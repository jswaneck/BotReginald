    "reginald",
    "discord"
  ],
  "author": "Jacob Swaneck + friends",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jswaneck/BotReginald/issues"
  },
  "homepage": "https://github.com/jswaneck/BotReginald#readme",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "discord.js": "13.1.0",
    "discord.js-multilingual-utils": "1.7.2",
    "discord.js-rate-limiter": "1.2.2",
    "express": "4.17.1",
    "express-promise-router": "4.1.0",
    "filesize": "8.0.0",
    "node-fetch": "2.6.1",
    "node-schedule": "2.0.0",
